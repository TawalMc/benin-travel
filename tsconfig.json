{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
		"baseUrl": "./src/",
		"paths": {
			"@/components/*":["components/*"],
			"@/utils/*":["utils/*"],
			"@/layouts/*":["layout/*"],
			"@/theme/*":["theme/*"],
			"@/data/*":["data/*"],
		}
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/utils/gtag.js"],
  "exclude": ["node_modules"]
}
